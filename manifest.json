{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Ukulele Tab Player",
  "version": "2.0",
  "permissions": [
    "activeTab", "tabs"
  ],
  
  "background": {
    "scripts": ["notes.js", "noteplayer.js", "songplayer.js", "ukuleletabplayer.js"],
    "persistent": false
  },
  
  "browser_action": {
    "default_title": "Ukulele Player",
    "default_icon": "uke_icon.png"
  },
  
  "manifest_version": 2
}
